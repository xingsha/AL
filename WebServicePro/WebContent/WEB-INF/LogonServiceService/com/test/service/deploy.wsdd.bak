<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from LogonServiceService WSDL service -->

  <service name="LogonService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.test.com"/>
      <parameter name="wsdlServiceElement" value="LogonServiceService"/>
      <parameter name="schemaQualified" value="http://service.test.com"/>
      <parameter name="wsdlServicePort" value="LogonService"/>
      <parameter name="className" value="com.test.service.LogonServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="LogonService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="checkUserRight" qname="operNS:checkUserRight" xmlns:operNS="http://service.test.com" soapAction="" >
        <parameter qname="pns:userId" xmlns:pns="http://service.test.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://service.test.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="checkUserRight"/>

  </service>
</deployment>
